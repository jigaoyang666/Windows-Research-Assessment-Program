<Window x:Class="RAP.View.PositionsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RAP"
        mc:Ignorable="d"
        Title="Positions" Height="450" Width="400" MinWidth="400" MinHeight="450">
    <Viewbox>
    <Grid x:Name="NewWindow">

        <DataGrid x:Name="positionList" ItemsSource="{Binding}" HorizontalAlignment="Left" Height="400" Margin="10,10,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2" IsReadOnly="True" />

    </Grid>
    </Viewbox>
</Window>
